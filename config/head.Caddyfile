*.vikiansiklopedi.org {
    tls {
        dns ${dns_provider}
    }
}

*.m.vikiansiklopedi.org {
    tls {
        dns ${dns_provider}
    }
}

vikiansiklopedi.org {
    proxy / https://cristiancantoro.github.io/wikiproxy/ {
        import config.d/proxy.Caddyfile
    }

    ratelimit * / 50 100 second
    import config.d/log.Caddyfile
}

