*.${domain} {
    tls {$$TLS_EMAIL}
    tls {
        dns ${dns_provider}
    }
}

*.m.${domain} {
    tls {
        dns ${dns_provider}
    }
}

${domain} {
    redir {
        / https://cristiancantoro.github.io/wikiproxy/ 303
    }

    ratelimit * / 50 100 second
    import config.d/log.Caddyfile
}

