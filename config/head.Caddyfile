*.vikiansiklopedi.org {
    tls {$$TLS_EMAIL}
    tls {
        dns ${dns_provider}
    }
}

*.m.vikiansiklopedi.org {
    tls {
        dns ${dns_provider}
    }
}

vikiansiklopedi.org {
    redir {
        / https://cristiancantoro.github.io/wikiproxy/ 303
    }

    ratelimit * / 50 100 second
    import config.d/log.Caddyfile
}

